<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>HalloJoe.RobotsTxt</AssemblyName>
    <RootNamespace>HalloJoe.RobotsTxt</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A tool for accessing and querying the contents of a robots.txt file.</Description>
    <Copyright>Casper Korsgaard</Copyright>
    <PackageProjectUrl>https://github.com/hallojoe/RobotsTxt</PackageProjectUrl>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <PackageLicenseUrl>https://github.com/hallojoe/RobotsTxt/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/hallojoe/RobotsTxt.git</RepositoryUrl>
    <PackageTags>robots.txt robotstxt</PackageTags>
    <Version>0.1.0</Version>
  </PropertyGroup>

</Project>
